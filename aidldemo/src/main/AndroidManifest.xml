<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wuxianedu.aidldemo">

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name="com.wuxianedu.aidldemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".service.CalculateService"
            android:enabled="true"
            android:exported="true"/>

        <service android:name=".service.DDService"
            android:enabled="true"
            android:exported="true"/>

        <service android:name=".service.BookService"
            android:enabled="true"
            android:exported="true"/>
    </application>

</manifest>